<?xml version="1.0" ?>
<!-- 前台配置   -->
<configuration>
<datagrid>
	<element label="站点名称" field="stationname" width="150"/>
	<element label="降雨量" field="rain" width="120"/>
</datagrid> 
<!-- shp服务工作空间 -->
<gisSpace>MyShapefileWorkspaceID</gisSpace>
<!-- 动态图层服务地址 -->
<urlRadarService>http://192.168.1.237:6080/arcgis/rest/services/cd/MapServer/dynamicLayer</urlRadarService>
<legend>
