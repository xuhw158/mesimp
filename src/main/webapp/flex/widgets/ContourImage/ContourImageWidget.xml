<?xml version="1.0"?>
<!-- 前台配置   -->
<configuration>
<types>
<type name="温度" typeNum="1" legend="assets/radar/temp.png"/>
<type name="降水" typeNum="2" legend="assets/radar/rain.png"/>
<type name="风力" typeNum="3" legend="assets/radar/wind.png"/>
</types>
<timeTypes>
<type name="上午" typeNum="0" />
<type name="下午" typeNum="12" />
</timeTypes>
<hours>
<hour name="24" />
<hour name="48"/>
<hour name="72"/>
<hour name="96"/>
</hours>
<colors>
		<type name="温度">
			 <class value="1"   range="-100~-20"  color="81,151,223"/>
		      <class value="2"   range="-20~-16"   color="126,175,233"/>
		      <class value="3"   range="-16~-12"   color="97,137,136"/>
		      <class value="4"   range="-12~-8"    color="118,221,228"/>
		      <class value="5"   range="-8~-4"     color="176,254,192"/>
		      <class value="6"   range="-4~0"      color="202,252,223"/>
		      <class value="7"   range="0~4"       color="254,254,187"/>
		      <class value="8"   range="4~8"       color="76,255,0"/>
		      <class value="9"   range="8~12"      color="251,228,126"/>
		      <class value="10"  range="12~16"     color="255,254,5"/>
		      <class value="11"  range="16~20"     color="249,167,70"/>
		      <class value="12"  range="20~24"     color="251,111,0"/>
		      <class value="13"  range="24~28"     color="251,183,180"/>
		      <class value="14"  range="28~32"     color="196,135,135"/>
		      <class value="15"  range="32~36"     color="249,145,255"/>
		      <class value="16"  range="36~40"     color="148,2,129"/>
		      <class value="17"  range="40~100"    color="225,0,0"/>
		 </type>
		<type name="降水">
		     <class value="1"   range="0~0.1"    color="255,255,255"/>
		      <class value="2"   range="0.1~2"    color="165,239,144"/>
		      <class value="3"   range="2~4"      color="59,165,7"/>
		      <class value="4"   range="4~6"      color="95,183,255"/>
		      <class value="5"   range="6~8"      color="5,0,252"/>
		      <class value="6"   range="8~10"     color="0,115,76"/>
		      <class value="7"   range="10~20"    color="251,1,246"/>
		      <class value="8"   range="20~50"    color="243,74,0"/>
		      <class value="9"   range="50~2000"  color="115,0,2"/>	
		</type>
		<type name="风力">
		     <class value="1"   range="0~0.1"    color="255,255,255"/>
		      <class value="2"   range="0.1~2"    color="165,239,144"/>
		      <class value="3"   range="2~4"      color="59,165,7"/>
		      <class value="4"   range="4~6"      color="95,183,255"/>
		      <class value="5"   range="6~8"      color="5,0,252"/>
		      <class value="6"   range="8~10"     color="0,115,76"/>
		      <class value="7"   range="10~20"    color="251,1,246"/>
		      <class value="8"   range="20~50"    color="243,74,0"/>
		      <class value="9"   range="50~2000"  color="115,0,2"/>	
		</type>
</colors>
<!-- 是否显示等值面值 true false -->
<showValue>false</showValue>
<!-- 播放间隔 单位：秒 -->
<timer>0.7</timer>
<!-- 色斑图文件服务器 -->
<CloudServer>http://localhost:8080/</CloudServer>
</configuration>